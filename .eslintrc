{
  "parser": "babel-eslint",

  "plugins": [
    "react"
  ],

  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },

  "extends": "airbnb",

  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },

  "rules": {
    "semi": [2, "never"],
    "func-names": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-return-assign": 0
  }
}
